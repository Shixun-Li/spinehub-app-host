{
  "name": "spinehub",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset:project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "sync:backend": "npx openapi-typescript@6.7.6 https://api.development.spinehub.link/api/swagger-json -o ./backend-sdk/schema.d.ts",
    "check:deps": "npx expo install --check",
    "env:pull": "moonenv pull spinehub-react-native",
    "env:push": "moonenv push spinehub-react-native",
    "prebuild:ios": "npx expo prebuild -p ios",
    "prebuild:android": "npx expo prebuild -p android",
    "submit:ios": "eas submit --platform ios",
    "eas:build:preview": "eas build --profile preview --platform",
    "eas:build:prod": "eas build --profile production --platform",
    "type-check": "npx tsc --noEmit"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@cometchat/chat-uikit-react-native": "^4.3.25",
    "@expo/vector-icons": "^14.0.0",
    "@hookform/resolvers": "^4.1.3",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-picker/picker": "2.11.0",
    "@react-navigation/native": "^7.0.14",
    "@shopify/flash-list": "1.7.6",
    "@tanstack/react-query": "^5.40.1",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "cookie": "^1.0.2",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "expo": "^53.0.7",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-document-picker": "~13.1.5",
    "expo-font": "~13.3.1",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.4",
    "expo-router": "~5.0.6",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.7",
    "expo-updates": "~0.28.12",
    "expo-web-browser": "~14.1.6",
    "fs-extra": "^11.3.0",
    "lightningcss": "^1.29.3",
    "openapi-fetch": "^0.13.5",
    "openapi-typescript-fetch": "^2.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.55.0",
    "react-native": "0.79.2",
    "react-native-autocomplete-input": "^5.5.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-shadow-2": "^7.1.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "yup": "^1.6.1",
    "zod": "^3.24.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "19.0.0",
    "cspell": "^8.15.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.5",
    "react-test-renderer": "19.0.0",
    "reactotron-react-native": "^5.1.13",
    "typescript": "~5.8.3"
  },
  "private": true,
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
